# MCP Server API Keys
# Copy this file to .env and fill in your actual keys

# Jina AI - For web search and content extraction (jino-agent)
# Get your key at: https://jina.ai
JINA_API_KEY=jina_your_api_key_here

# Notion - For Notion integration
# Get your key at: https://www.notion.so/my-integrations
NOTION_API_TOKEN=secret_your_notion_token_here

# 1Password (Optional) - For secrets management
# Get service account at: https://1password.com/products/secrets-management
# OP_SERVICE_ACCOUNT_TOKEN=ops_your_service_account_token_here
# E2B Cloud API Key
# Get your API key from https://e2b.dev/dashboard
E2B_API_KEY=your_e2b_api_key_here

# GitHub Token for Copilot CLI and MCP GitHub tools
# Generate at https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Optional: Additional MCP Server API Keys
JINA_API_KEY=your_jina_api_key_here
NOTION_API_KEY=your_notion_api_key_here
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Optional: Anthropic API Key for Claude integration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Sandbox Configuration
E2B_SANDBOX_TIMEOUT=300000
E2B_SANDBOX_TEMPLATE=base
